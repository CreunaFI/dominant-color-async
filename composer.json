{
    "name": "joppuyo/dominant-color-async",
    "type": "wordpress-plugin",
    "description": "Calculate the dominant color for every image in WordPress, asynchronously",
    "license": "GPL-3.0-or-later",
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "5.5.9"
        }
    },
    "require": {
        "a5hleyrich/wp-background-processing": "^1.0",
        "ksubileau/color-thief-php": "^1.4",
        "intervention/image": "^2.4",
        "tightenco/collect": "~5.2",
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "DominantColorAsync\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    }
}
